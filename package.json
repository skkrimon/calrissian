{
  "name": "calrissian",
  "version": "0.1.0",
  "description": "Tauri App for managing Lando Envs",
  "private": false,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "npx eslint src/**/*.{ts,tsx}",
    "lint:fix": "npx eslint --fix src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,css} --config ./.prettierrc",
    "ca": "pnpm run lint:fix && pnpm run format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skkrimon/calrissian.git"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@microlink/react-json-view": "^1.22.2",
    "@mui/material": "^5.10.4",
    "@tauri-apps/api": "^1.0.2",
    "dirty-json": "^0.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.1.1",
    "@types/node": "^18.7.10",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.2"
  },
  "keywords": [
    "react",
    "tauri",
    "typescript",
    "rust",
    "lando",
    "calrissian"
  ],
  "author": "Simon Zapf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skkrimon/calrissian/issues"
  }
}
